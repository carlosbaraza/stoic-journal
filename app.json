{
  "expo": {
    "name": "Stoic Journal: Moral, Daily routine",
    "slug": "StoicJournal",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EAD2AC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baraza.stoicjournal",
      "buildNumber": "1.0.0",
      "icon": "./assets/icon-ios.png"
    },
    "description": "",
    "githubUrl": "https://github.com/carlosbaraza/stoic-journal",
    "android": {
      "package": "com.baraza.stoicjournal",
      "icon": "./assets/icon-android.png",
      "versionCode": 1,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "USE_FINGERPRINT",
        "VIBRATE",
        "WRITE_EXTERNAL_STORAGE",
        "com.android.launcher.permission.INSTALL_SHORTCUT"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#222"
    }
  }
}
